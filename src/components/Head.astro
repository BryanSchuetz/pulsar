---
const {title} = Astro.props
import { ViewTransitions } from 'astro:transitions';
import '../global.css';
---

<head>

  <ViewTransitions />
  <meta charset="utf-8" />
  <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
  <meta name="viewport" content="width=device-width" />
  <title>{title}</title>
</head>